# Configurações do Banco de Dados
DB_HOST=localhost
DB_PORT=5432
DB_NAME=sistema_imobiliario
DB_USER=imobiliaria_user
DB_PASSWORD=sua_senha_postgresql_aqui

# JWT Secret (use uma chave forte em produção)
JWT_SECRET=sua_chave_jwt_muito_forte_e_segura_aqui

# Configurações do Servidor
PORT=3000
NODE_ENV=development
FRONTEND_URL=http://localhost:3000

# Redis (opcional, para cache)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=sua_senha_redis_aqui

# ===================================
# CONFIGURAÇÕES PARA PRODUÇÃO/VPS
# ===================================

# Configurações Docker
COMPOSE_PROJECT_NAME=sistema-imobiliario
POSTGRES_PASSWORD=senha_postgres_docker
REDIS_PASSWORD=senha_redis_docker
